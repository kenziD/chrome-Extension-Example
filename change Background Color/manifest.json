{
  "manifest_version": 2,
  "name": "change the color",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "background":{
  	"scripts":["background.js"],
  	"persistent":false
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["myscript.js"]
    }
  ],
  "browser_action": {
  	"default_title": "change background color",
    "default_icon": "background.png"
    // "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ]
}